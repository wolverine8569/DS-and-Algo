// we can do two operations 
1. divide element by 2 if no. is even
2. add 1 or substract 1 from  number

while(t--){
    long a,count=0;
    cin>>a;
    while(a>1){
        if(a%2==0)
        {
            a/=2;
        }
        else if(a%4==1 || a==3)
        a--;
        else
        a++;
        count++;
    }
    cout<<count<<'\n';
